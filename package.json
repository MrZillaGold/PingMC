{
  "name": "pingmc",
  "version": "2.1.1",
  "author": "MrZillaGold <Pronin Egor>",
  "description": "Promise-based Minecraft pinger in Node.js",
  "license": "MIT",
  "scripts": {
    "build": "babel lib --out-dir dist && node --experimental-modules --es-module-specifier-resolution=node ./scripts/fixImports.mjs"
  },
  "main": "./dist/PingMC.js",
  "exports": {
    ".": {
      "require": "./dist/PingMC.js",
      "default": "./lib/PingMC.mjs"
    }
  },
  "module": "lib/PingMC.mjs",
  "keywords": [
    "ping",
    "minecraft",
    "server",
    "query",
    "srv"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrZillaGold/PingMC"
  },
  "dependencies": {
    "node-int64": "^0.4.0",
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1"
  }
}
